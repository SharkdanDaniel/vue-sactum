    "components": {
        "schemas": {
            "LoginRequest": {
                "title": "Login",
                "description": "Login request",
                "properties": {
                    "email": {
                        "description": "Autentication email",
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "description": "Autentication password",
                        "type": "string",
                        "format": "password"
                    }
                },
                "type": "object"
            },
            "StoreProductRequest": {
                "title": "Product",
                "description": "Crud product",
                "properties": {
                    "name": {
                        "description": "Product name",
                        "type": "string"
                    },
                    "price": {
                        "description": "Product price",
                        "type": "number",
                        "format": "float",
                        "default": 487.65
                    },
                    "description": {
                        "description": "Product description",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "StoreUserRequest": {
                "title": "User",
                "description": "Crud user",
                "properties": {
                    "name": {
                        "description": "user name",
                        "type": "string"
                    },
                    "email": {
                        "description": "User email",
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "description": "User password",
                        "type": "string",
                        "format": "password"
                    }
                },
                "type": "object"
            },
            "ApiPaginatorResponse": {
                "title": "Api paginator response",
                "description": "Api paginator response",
                "properties": {
                    "total": {
                        "description": "Data total in Database",
                        "type": "integer",
                        "default": 50
                    },
                    "per_page": {
                        "description": "Total of data per page",
                        "type": "integer",
                        "default": 15
                    },
                    "current_page": {
                        "description": "Current page number",
                        "type": "integer",
                        "default": 1
                    },
                    "last_page": {
                        "description": "Last page number number",
                        "type": "integer",
                        "default": 4
                    },
                    "first_page_url": {
                        "description": "Url of the first page",
                        "type": "string",
                        "default": "http://laravel.app?page=1"
                    },
                    "last_page_url": {
                        "description": "Url of the last page",
                        "type": "string",
                        "default": "http://laravel.app?page=4"
                    },
                    "next_page_url": {
                        "description": "Url of the next page",
                        "type": "string",
                        "default": "http://laravel.app?page=2"
                    },
                    "prev_page_url": {
                        "description": "Url of the previous page",
                        "type": "string",
                        "default": null
                    },
                    "path": {
                        "description": "App path",
                        "type": "string",
                        "default": "http://laravel.app"
                    },
                    "from": {
                        "description": "From number",
                        "type": "integer",
                        "default": 1
                    },
                    "to": {
                        "description": "To number",
                        "type": "integer",
                        "default": 15
                    }
                },
                "type": "object"
            },
            "ApiResponse": {
                "title": "Api response",
                "description": "Api response",
                "properties": {
                    "code": {
                        "description": "Status code",
                        "type": "integer",
                        "default": 200
                    },
                    "type": {
                        "description": "Response type",
                        "type": "string"
                    },
                    "message": {
                        "description": "Reponse message",
                        "type": "string",
                        "default": "API message response"
                    }
                },
                "type": "object"
            },
            "LoginResponse": {
                "title": "Login response",
                "description": "Login response",
                "properties": {
                    "access_token": {
                        "description": "Token",
                        "type": "string",
                        "default": "1|tYfXgGERi0gcGH7Vdu6TYdWdijCCdrdPjY6jGBphAnwx"
                    },
                    "token_type": {
                        "description": "Token type",
                        "type": "string",
                        "default": "bearer"
                    },
                    "user": {
                        "$ref": "#/components/schemas/User"
                    }
                },
                "type": "object"
            },
            "Product": {
                "title": "Product model",
                "description": "Product model",
                "properties": {
                    "id": {
                        "description": "Product ID",
                        "type": "string",
                        "format": "uuid"
                    },
                    "name": {
                        "description": "Product name",
                        "type": "string"
                    },
                    "price": {
                        "description": "Product price",
                        "type": "number",
                        "format": "float",
                        "default": 451.45
                    },
                    "description": {
                        "description": "Product description",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "ProductList": {
                "title": "ProductList",
                "description": "ProductList",
                "allOf": [
                    {
                        "$ref": "#/components/schemas/ApiPaginatorResponse"
                    },
                    {
                        "properties": {
                            "data": {
                                "description": "Product list",
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/Product"
                                }
                            }
                        }
                    }
                ]
            },
            "User": {
                "title": "User model",
                "description": "User model",
                "properties": {
                    "id": {
                        "description": "User ID",
                        "type": "string",
                        "default": 48
                    },
                    "name": {
                        "description": "user name",
                        "type": "string"
                    },
                    "email": {
                        "description": "User email",
                        "type": "string",
                        "format": "email"
                    }
                },
                "type": "object"
            },
            "UserList": {
                "title": "UserList",
                "description": "UserList",
                "allOf": [
                    {
                        "$ref": "#/components/schemas/ApiPaginatorResponse"
                    },
                    {
                        "properties": {
                            "data": {
                                "description": "User list",
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    }
                ]
            }
        }
    }
}